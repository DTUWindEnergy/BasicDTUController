begin simulation;
  time_stop    900;
  solvertype   1 ;
  on_no_convergence continue ;
  convergence_limits 1E3 1.0 1E-7 ;
  logfile ./logfiles/dtu10mw_with_actuators.log ;
  begin newmark;
    deltat    0.02;
  end newmark;
end simulation;
